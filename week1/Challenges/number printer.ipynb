   "source": [
    "#Even/Odd Number Printer\n",
    "numbers=list(range(1,21))\n",
    "\n",
    "for num in numbers:\n",
    "    if (num%2) == 0:\n",
    "        print(num, 'is an even number')\n",
    "    else:\n",
    "        print(num,'is an odd number')"
